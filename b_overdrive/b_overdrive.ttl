@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix lv2: <http://lv2plug.in/ns/lv2core#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://gareus.org/rgareus#me>
	a foaf:Person ;
	foaf:name "Robin Gareus" ;
	foaf:mbox <mailto:robin@gareus.org> ;
	foaf:homepage <http://gareus.org/> .

<http://gareus.org/oss/lv2/b_overdrive>
	a doap:Project, lv2:Plugin, lv2:DistortionPlugin;
	doap:maintainer <http://gareus.org/rgareus#me> ;
	doap:name "B Organ Overdrive";
	doap:license <http://usefulinc.com/doap/licenses/gpl> ;
	lv2:optionalFeature lv2:hardRTCapable ;
	lv2:port
	[
		a lv2:AudioPort ,
			lv2:InputPort ;
		lv2:index 0 ;
		lv2:symbol "in" ;
		lv2:name "In"
	],	
	[
		a lv2:AudioPort ,
			lv2:OutputPort ;
		lv2:index 1 ;
		lv2:symbol "out" ;
		lv2:name "Out"
	],
	[
		a lv2:InputPort ,
			lv2:ControlPort ;
		lv2:index 2 ;
		lv2:symbol "bias" ;
		lv2:name "Bias";
		lv2:default 0.87399;
		lv2:minimum 0.0 ;
		lv2:maximum 1.0 ;
	],
	[
		a lv2:InputPort ,
			lv2:ControlPort ;
		lv2:index 3 ;
		lv2:symbol "feedback" ;
		lv2:name "Feedback";
		lv2:default 0.5821;
		lv2:minimum 0.0 ;
		lv2:maximum 1.0 ;
	],
	[
		a lv2:InputPort ,
			lv2:ControlPort ;
		lv2:index 4 ;
		lv2:symbol "sagtobias" ;
		lv2:name "SagToBias";
		lv2:default 0.1880;
		lv2:minimum 0.0 ;
		lv2:maximum 1.0 ;
	],
	[
		a lv2:InputPort ,
			lv2:ControlPort ;
		lv2:index 5 ;
		lv2:symbol "postfeed" ;
		lv2:name "Postdiff feedback";
		lv2:default 1.0 ;
		lv2:minimum 0.0 ;
		lv2:maximum 1.0 ;
	],
	[
		a lv2:InputPort ,
			lv2:ControlPort ;
		lv2:index 6 ;
		lv2:symbol "globfeed" ;
		lv2:name "Global feedback";
		lv2:default 0.5826;
		lv2:minimum 0.0 ;
		lv2:maximum 1.0 ;
	],
	[
		a lv2:InputPort ,
			lv2:ControlPort ;
		lv2:index 7 ;
		lv2:symbol "gainin" ;
		lv2:name "Input Gain";
		lv2:default 0.3567;
		lv2:minimum 0.0 ;
		lv2:maximum 1.0 ;
	],
	[
		a lv2:InputPort ,
			lv2:ControlPort ;
		lv2:index 8 ;
		lv2:symbol "gainout" ;
		lv2:name "Output Gain";
		lv2:default .07873;
		lv2:minimum 0.0 ;
		lv2:maximum 1.0 ;
	]
	.
